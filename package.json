{
  "name": "ts-cached-api-client",
  "version": "1.0.0",
  "description": "axios wrapper with ability to cache responses on different application layers",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CapitanFindusFI/ts-cached-api-client.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "cache"
  ],
  "author": "Andrea Martini <am95.dev@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CapitanFindusFI/ts-cached-api-client/issues"
  },
  "homepage": "https://github.com/CapitanFindusFI/ts-cached-api-client#readme"
}
